{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "globalEnv": [
    "NEXT_PUBLIC_HOST",
    "NEXT_PUBLIC_EMAIL_INFO",
    "NEXT_PUBLIC_EMAIL_NOREPLY",
    "NEXT_PUBLIC_EMAIL_TRAINING",
    "NEXT_PUBLIC_SUPABASE_URL",
    "NEXT_PUBLIC_SUPABASE_ANON_KEY",
    "NEXT_RESEND_KEY_GENERAL",
    "NEXT_RESEND_AUDIENCE_ID_GENERAL",
    "NEXT_RESEND_EMAIL",
    "JWT_SECRET",
    "NEXT_PUBLIC_REST_COUNTRIES_API_URL",
    "NEXT_PUBLIC_CLAUDE_MODEL",
    "NEXT_ANTHROPIC_KEY",
    "NEXT_PUBLIC_MAILCHIMP_SIGNUP_URL",
    "NEXT_PUBLIC_MAILCHIMP_SERVERNAME_PREFIX",
    "NEXT_MAILCHIMP_AUDIENCE_ID_DST",
    "NEXT_MAILCHIMP_KEY_GENERAL"
  ],
  "tasks": {
    "start": {
      "dependsOn": ["^start"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"],
      "env": [
        "NEXT_PUBLIC_HOST",
        "NEXT_PUBLIC_EMAIL_INFO",
        "NEXT_PUBLIC_EMAIL_NOREPLY",
        "NEXT_PUBLIC_EMAIL_TRAINING",
        "NEXT_PUBLIC_SUPABASE_URL",
        "NEXT_PUBLIC_SUPABASE_ANON_KEY",
        "NEXT_RESEND_KEY_GENERAL",
        "NEXT_RESEND_AUDIENCE_ID_GENERAL",
        "NEXT_RESEND_EMAIL",
        "JWT_SECRET",
        "NEXT_PUBLIC_REST_COUNTRIES_API_URL",
        "NEXT_PUBLIC_CLAUDE_MODEL",
        "NEXT_ANTHROPIC_KEY",
        "NEXT_PUBLIC_MAILCHIMP_SIGNUP_URL",
        "NEXT_PUBLIC_MAILCHIMP_SERVERNAME_PREFIX",
        "NEXT_MAILCHIMP_AUDIENCE_ID_DST",
        "NEXT_MAILCHIMP_KEY_GENERAL"
      ]
    },
    "types#build": {
      "dependsOn": ["enums#build"]
    },
    "lint": {
      "dependsOn": ["^lint"],
      "outputs": []
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "format": {
      "dependsOn": ["^format"]
    },
    "check-format": {
      "dependsOn": ["^check-format"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    }
  }
}
